body {
  background-image: url(../images/winterfest.png);
  background-repeat: repeat;
}

.profileModule {
  border: 2px double #BDC8D7;
  background-color: #EEE9EE;
  padding: 15px;
  margin: 10px;
  color: #C4C0CD;
  text-align: center;
  letter-spacing:-1px;
  border-radius:5px;
}

.moduleTitle {
  color: whitesmoke;
}

.title {
  color: #73b1fe;
  text-align: right;
}

.data {
  text-align: left;
}

#tag {
  text-align: center;
}

#character {
  text-align: center;
}

#leftProfile {
  background-color: #5e6697;
  text-align: center;
}

#rightProfile {
  background-color: #5e6697;
  text-align: center;
}

#topProfile {
  background-color: #5e6697;
  text-align: center;
}

a:hover {
  color: #dbf1ff;
}

#tag img, #profileHA {
  padding: 0px;
  margin: 3px;
  border: 2px double black;
}

.moduleTitle {
  line-height: 25px;
}

#middle .profileBox {
  background-color:transparent;
}

a:link, a:visited {
  color: white;
  font-weight: bold;
  font-variant: small-caps;
  text-decoration: none;
}

a:hover   {
  color: #FBEFF6;
}

.moduleTitle {
text-decoration: underline;
font-style:italic;
font-family: 'Times New Roman', cursive;
text-transform: lowercase;
font-size:18px;
}
